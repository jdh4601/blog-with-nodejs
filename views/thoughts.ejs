<section class="articles">
  <h2 class="articles__heading">Thoughts</h2>
  <ul class="article-ul">
    <% data.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="article-list__date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>
    <% }) %>
  </ul>

  <% if (nextPage !== null) { %>
  <a href="thoughts/?page=<%= nextPage %>" class="pagination">&lt; Previous</a>
  <% } %>
</section>
